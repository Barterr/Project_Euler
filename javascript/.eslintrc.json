{
  "plugins": [
    "prettier"
	],
	"extends": [
    "prettier"
  ],
	"parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
			"error",
			{
				"printWidth": 120,
				"tabWidth": 2,
				"singleQuote": true,
				"trailingComma": "none",
				"bracketSpacing": true,
				"semi": false,
				"useTabs": false,
				"parser": "babylon",
				"jsxBracketSameLine": false
			}
		]
	}
}